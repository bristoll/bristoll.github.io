gemspec
group :jekyll_plugins do
    gem 'jekyll-feed'
    gem 'jekyll-paginate'
    gem 'jekyll-seo-tag'
    gem 'jekyll-sitemap'
end
gem "jekyll", ENV["JEKYLL_VERSION"] if ENV["JEKYLL_VERSION"]
gem "kramdown-parser-gfm" if ENV["JEKYLL_VERSION"] == "~> 3.9"
